    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.0",
    "rimraf": "^3.0.2"
  },
  "optionalDependencies": {
